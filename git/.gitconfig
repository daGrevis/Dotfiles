[user]
	name = Raitis Stengrevics
	email = dagrevis@gmail.com
[core]
	pager = less -+F
	excludesfile = ~/.gitignore_global
[color]
	ui = true
	diff = always
[rerere]
	enabled = true
[merge]
	tool = vimdiff
[github]
	user = daGrevis
[push]
	default = current
[grep]
	extendRegexp = true
	lineNumber = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
